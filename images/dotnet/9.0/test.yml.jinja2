schemaVersion: 2.0.0
commandTests:
  - name: "dotnet-installed"
    command: "which"
    args: ["dotnet"]
    expectedOutput: ["/usr/local/bin/dotnet"]
  - name: "dotnet-version"
    command: "dotnet"
    args: ["--version"]
    expectedOutput: ["{{ "dotnet-sdk" | resolve_version | replace("xx", ".*") }}"]
